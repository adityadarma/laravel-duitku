{
    "name": "adityadarma/laravel-duitku",
    "description": "Library payment gateway duitku for laravel",
    "homepage": "https://github.com/adityadarma/laravel-duitku",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aditya Darma",
            "email": "adhit.boys1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
		"laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AdityaDarma\\LaravelDuitku\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdityaDarma\\LaravelDuitku\\LaravelDuitkuServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
